  "author": "Peace Thabiwa",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.11.0",
    "googleapis": "^127.0.0",
    "handlebars": "^4.7.8"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/handlebars": "^4.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.0"
  }
}
